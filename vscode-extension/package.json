{
  "publisher": "antfu",
  "name": "ext-name",
  "displayName": "ext-name",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@10.27.0",
  "description": "",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/ext-name#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/antfu/ext-name"
  },
  "bugs": {
    "url": "https://github.com/antfu/ext-name/issues"
  },
  "sponsor": {
    "url": "https://github.com/sponsors/antfu"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.cjs",
  "icon": "res/icon.png",
  "files": [
    "LICENSE.md",
    "dist/*",
    "res/*"
  ],
  "engines": {
    "vscode": "^1.97.0"
  },
  "activationEvents": [],
  "contributes": {
    "commands": [],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "metabase",
          "title": "Metabase",
          "icon": "res/sidebar-icon.svg"
        }
      ]
    },
    "views": {
      "metabase": [
        {
          "id": "metabase.content",
          "name": "Content"
        },
        {
          "id": "metabase.dataCatalog",
          "name": "Data Catalog"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "metabase.content",
        "contents": "Open a folder with Metabase content exported via Remote Sync.",
        "when": "!metabase.configExists"
      },
      {
        "view": "metabase.dataCatalog",
        "contents": "Open a folder with Metabase content exported via Remote Sync.",
        "when": "!metabase.configExists"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "ext-name",
      "properties": {}
    }
  },
  "scripts": {
    "build": "nr build:ext && nr build:webview",
    "build:ext": "tsdown src/index.ts --external vscode",
    "build:webview": "cd webview && vite build",
    "dev": "nr build:ext --watch --sourcemap",
    "dev:webview": "cd webview && vite",
    "prepare": "nr update",
    "update": "vscode-ext-gen --output src/generated/meta.ts",
    "lint": "eslint .",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "vscode:prepublish": "nr build",
    "release": "bumpp",
    "ext:package": "vsce package --no-dependencies",
    "ext:publish": "vsxpub --no-dependencies"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.7.3",
    "@antfu/ni": "^28.0.0",
    "@types/node": "^25.0.3",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/vscode": "^1.97.0",
    "@vitejs/plugin-react": "^4.4.0",
    "@vscode/vsce": "^3.7.1",
    "bumpp": "^10.3.2",
    "eslint": "^9.39.2",
    "ovsx": "^0.10.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "reactive-vscode": "^1.0.0-beta.2",
    "taze": "^19.9.2",
    "tsdown": "^0.19.0-beta.3",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vitest": "^4.0.16",
    "vscode-ext-gen": "^1.5.1",
    "vsxpub": "^0.1.2",
    "yaml": "^2.8.2"
  }
}
