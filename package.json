  "scripts": {
    "mage": "./bin/mage",
    "build": "bun run build:cljs && bun run build:js",
    "build-embedding-sdk-package": "bun run embedding-sdk:clean:dist && bun install && bun run build-release:cljs && bun run embedding-sdk:generate-package && bun run embedding-sdk:generate-nextjs-compat && bun run build-release:embedding-sdk-package && bun run build-embedding-sdk-cli && bun run embedding-sdk:dts:generate",
    "build-embedding-sdk-cli": "rspack --config rspack.embedding-sdk-cli.config.js",
    "build-embedding-sdk-cli:watch": "bun run build-embedding-sdk-cli --watch",
    "build-hot": "rm -f target/cljs_dev/cljs.core.js && bun run build:cljs && bun run concurrently -n 'js' -c 'green,cyan' 'bun run build-hot:js'",
    "build-hot-be": "rm -f target/cljs_dev/cljs.core.js && bun run concurrently -n 'cljs,js' -c 'yellow,green,cyan' 'bun run build-hot:cljs' 'ENABLE_CLJS_HOT_RELOAD=true bun run build-hot:js-wait'",
    "build-hot:cljs": "bun install && shadow-cljs watch app",
    "build-hot:js": "bun install && bun run clean-dev:js && NODE_OPTIONS=--max-old-space-size=8192 WEBPACK_BUNDLE=hot rspack serve",
    "build-hot:js-wait": "bun run wait:cljs; bun run build-hot:js",
    "build-pure:cljs": "bun run clean-dev:cljs && shadow-cljs compile app",
    "build-release": "bun run build-release:cljs && bun run build-release:js",
    "build-release:cljs": "bun run clean-release:cljs && shadow-cljs release app",
    "build-release:js": "WEBPACK_BUNDLE=production rspack build",
    "build-release:static-viz": "WEBPACK_BUNDLE=production rspack --config rspack.static-viz.config.js",
    "build-release:embedding-sdk-package": "WEBPACK_BUNDLE=production rspack --config rspack.embedding-sdk-package.config.js",
    "build-release:embedding-sdk-bundle": "WEBPACK_BUNDLE=production rspack --config rspack.embedding-sdk-bundle.config.js",
    "build-shared": "bun install && rspack --config rspack.shared.config.js",
    "build-static-viz": "bun install && bun run build-release:cljs && WEBPACK_BUNDLE=production rspack --config rspack.static-viz.config.js",
    "build-static-viz:watch": "rspack --config rspack.static-viz.config.js --watch",
    "build-static-viz:watch-wait": "bun run wait:cljs; bun run build-static-viz:watch",
    "build-stats": "bun install && rspack --json > stats.json",
    "build-storybook": "NODE_OPTIONS=--max-old-space-size=8192 storybook build",
    "build-watch": "bun run concurrently -n 'cljs,js' 'bun run build-watch:cljs' 'bun run build-watch:js'",
    "build-watch:cljs": "bun install && shadow-cljs watch app",
    "build-watch:js": "bun install && NODE_OPTIONS=--max-old-space-size=8192 rspack serve --watch",
    "build:cljs": "bun install && bun run build-pure:cljs",
    "build:js": "bun install && rspack build",
    "chromatic": "bun run build:cljs && NODE_OPTIONS=--max-old-space-size=8192 chromatic",
    "ci": "bun run ci-frontend && bun run ci-backend",
    "ci-backend": "clojure -X:dev:ee:ee-dev:drivers:drivers-dev:eastwood && clojure -X:dev:test",
    "ci-frontend": "bun run lint && bun run test",
    "clean-cypress-artifacts": "rm -R -f cypress/",
    "clean-dev:cljs": "rm -rf target/cljs_dev/*",
    "clean-dev:js": "rm -rf resources/frontend_client/app/dist target/classes/frontend_client/app/dist",
    "clean-release:cljs": "rm -rf target/cljs_release/*",
    "clean:cljs": "bun install && bun run clean-dev:cljs && bun run clean-release:cljs",
    "cljs-nrepl": "bun install && shadow-cljs node-repl -d cider/cider-nrepl:0.57.0 -d cider/piggieback:0.6.1 -d refactor-nrepl/refactor-nrepl:3.11.0",
    "cljs-server-restart": "bun install && shadow-cljs restart",
    "cljs-server-start": "bun install && shadow-cljs start",
    "cljs-server-stop": "bun install && shadow-cljs stop",
    "concurrently": "bun install && concurrently --kill-others -p name",
    "dev": "bun install && bun run clean-dev:cljs && concurrently -n 'backend,frontend,cljs,static-viz' -c 'blue,green,yellow,magenta,red,cyan' 'clojure -M:run:dev:dev-start --hot' 'bun run build-hot:js-wait' 'bun run build:cljs' 'bun run build-static-viz:watch-wait'",
    "dev-hot-be": "bun install && bun run clean-dev:cljs && concurrently -n 'backend,frontend,cljs,static-viz' -c 'blue,green,yellow,magenta,red,cyan' 'clojure -M:run:dev:dev-start --hot' 'ENABLE_CLJS_HOT_RELOAD=true bun run build-hot:js-wait' 'bun run build-hot:cljs' 'bun run build-static-viz:watch-wait'",
    "dev-ee": "bun install && bun run clean-dev:cljs && concurrently -n 'backend,frontend,cljs,static-viz' -c 'blue,green,yellow,magenta,red,cyan' 'clojure -M:run:ee:dev:dev-start --hot' 'MB_EDITION=ee bun run build-hot:js-wait' 'MB_EDITION=ee bun run build:cljs' 'bun run build-static-viz:watch-wait'",
    "dev-ee-hot-be": "bun install && bun run clean-dev:cljs && concurrently -n 'backend,frontend,cljs,static-viz' -c 'blue,green,yellow,magenta,red,cyan' 'clojure -M:run:ee:dev:dev-start --hot' 'MB_EDITION=ee ENABLE_CLJS_HOT_RELOAD=true bun run build-hot:js-wait' 'MB_EDITION=ee bun run build-hot:cljs' 'bun run build-static-viz:watch-wait'",
    "embedding-sdk:generate-package": "node ./bin/embedding-sdk/generate-sdk-package-files.js",
    "embedding-sdk:publish": "cd ./resources/embedding-sdk && npm publish",
    "embedding-sdk:generate-nextjs-compat": "tsx enterprise/frontend/src/embedding-sdk-package/bin/generate-nextjs-compat.ts",
    "embedding-sdk:test-unit": "bun run test-unit enterprise/frontend/src/embedding-sdk-package/ enterprise/frontend/src/embedding-sdk-bundle/ enterprise/frontend/src/embedding-sdk-shared/",
    "embedding-sdk:dts:generate": "bun run embedding-sdk:clean:tsbuildinfo && bun run tsc --project tsconfig.sdk.json || true && bun run embedding-sdk:dts:fixup",
    "embedding-sdk:dts:fixup": "node ./bin/embedding-sdk/fixup-types-after-compilation.js",
    "embedding-sdk:docs:generate:pure": "typedoc --options ./.typedoc/typedoc.html.config.js && typedoc --options ./.typedoc/typedoc.markdown.config.js",
    "embedding-sdk:docs:generate": "bun run build-release:embedding-sdk-package && bun run embedding-sdk:dts:generate && bun run embedding-sdk:docs:generate:pure",
    "embedding-sdk:cli-snippets:type-check": "./bin/embedding-sdk/type-check-embedding-sdk-cli-snippets.sh",
    "embedding-sdk:docs-snippets:type-check": "./bin/embedding-sdk/type-check-embedding-sdk-docs-snippets.sh",
    "embedding-sdk:clean:dist": "rm -rf resources/embedding-sdk/dist",
    "embedding-sdk:clean:tsbuildinfo": "rm -rf resources/embedding-sdk/.tsbuildinfo",
    "embedding-sdk:dev": "EMBEDDING_SDK_BUNDLE_HOST=http://localhost:3100 concurrently -n 'serve-resources,tsc,webpack,fix-types' -c 'red,blue,green,yellow' 'bun run serve-resources' 'npm:embedding-sdk:dev:dts' 'npm:embedding-sdk:dev:webpack' 'npm:embedding-sdk:dev:fixup'",
    "embedding-sdk:dev:webpack": "bun run embedding-sdk:generate-package && bun run embedding-sdk:generate-nextjs-compat && WEBPACK_BUNDLE=development concurrently -n 'embedding-sdk-package,embedding-sdk-bundle' 'rspack --config rspack.embedding-sdk-package.config.js --watch' 'rspack --config rspack.embedding-sdk-bundle.config.js --watch'",
    "embedding-sdk:dev:dts": "tsc --project tsconfig.sdk.json --watch --preserveWatchOutput",
    "embedding-sdk:dev:fixup": "bun run embedding-sdk:dts:fixup --watch",
    "eslint-fix": "bun run lint-eslint --fix",
    "find-never-defined-css-variables": "tsx bin/find-never-defined-css-variables/find-never-defined-css-variables.ts",
    "generate-cypress-html-report": "mochawesome-merge cypress/reports/mochareports/*.json > cypress/reports/cypress-test-report.json && marge cypress/reports/cypress-test-report.json -o cypress/reports --inline",
    "generate-openapi": "clojure -M:run:ee generate-openapi-spec",
    "lint": "bun run lint-eslint && bun run lint-prettier && bun run lint-docs-links && bun run lint-yaml && bun run type-check",
    "lint-docs-links": "bun install && ./bin/verify-doc-links",
    "lint-css": "stylelint './frontend/**/*.css'",
    "lint-eslint": "bun run build:cljs && bun run lint-eslint-pure",
    "lint-eslint-pure": "eslint --cache --cache-strategy content --max-warnings 0 --report-unused-disable-directives enterprise/frontend frontend e2e",
    "lint-prettier": "bun install && bun lint-prettier-pure",
    "lint-prettier-pure": "prettier --check --cache '{frontend,enterprise/frontend,e2e}/**/*.{js,jsx,ts,tsx,css}'",
    "lint-yaml": "yamllint **/*.{yaml,yml} --ignore=node_modules/**/*.{yaml,yml}",
    "postinstall": "if [ -z \"$CI\" ]; then patch-package; fi",
    "precommit": "lint-staged",
    "preinstall": "node -e \"if(!/bun/.test(process.env.npm_execpath || '')) { console.error('Please use Bun, not npm or yarn.'); process.exit(1); }\"",
    "prepare": "husky",
    "prettier": "prettier --write --cache '{frontend,enterprise/frontend,e2e}/**/*.{js,jsx,ts,tsx,css}'",
    "remove-webpack-cache": "rm -rf ./node_modules/.cache",
    "serve-resources": "http-server resources/frontend_client -p 3100 -c-1",
    "storybook": "bun run build:cljs && storybook dev -p 6006",
    "storybook-pure": "storybook dev -p 6006",
    "storybook-embedding-sdk": "IS_EMBEDDING_SDK=true storybook dev -c frontend/src/embedding-sdk-shared/.storybook -p 6006",
    "test": "bun run test-unit && bun run test-timezones && bun run test-cypress",
    "test-cljs": "bun install && shadow-cljs compile test && node target/node-tests.js",
    "test-cypress": "tsx ./e2e/runner/run_cypress_local.ts",
    "test-cypress-host-sample-apps": "tsx ./e2e/runner/run_cypress_host_sample_apps.ts",
    "test-debug": "bun run build:cljs && node --inspect-brk node_modules/.bin/jest --runInBand --detectOpenHandles",
    "test-timezones": "bun install && ./frontend/test/__runner__/run_timezone_tests",
    "test-timezones-unit": "bun run build:cljs && jest --silent --maxWorkers=4 --config jest.tz.unit.conf.js",
    "test-unit": "bun run build:cljs && jest --maxWorkers=4",
    "test-unit-keep-cljs": "jest --maxWorkers=4",
    "test-unit-watch": "bun run test-unit --watch",
    "test-unit-watch:cljs": "bun run concurrently -n 'cljs,tests' 'bun run build-watch:cljs' 'bun run test-unit-keep-cljs --watch'",
    "test-visual:loki": "NODE_ENV=development loki test --chromeFlags='--headless --disable-gpu' ",
    "test-visual:loki:ci": "bun run build-storybook && bun run test-visual:loki --reactUri file:./storybook-static --verboseRenderer",
    "test-visual:loki-approve-diff": "ls .loki/difference | xargs -I _ find .loki/current -name _ | xargs -I _ cp _ .loki/reference/",
    "test-visual:loki-prune": "ls .loki/reference | grep -v \"$(ls .loki/current)\" | xargs -I {} rm .loki/reference/{}",
    "test-visual:loki-report": "node frontend/test/generate-loki-report-json.js && reg-cli --from .loki/report.json --report .loki/report.html",
    "test-visual:loki-report-open": "bun run test-visual:loki || (echo 'Visual test failed, opening report...' && bun run test-visual:loki-report && open-cli .loki/report.html)",
    "type-check": "bun run clean:cljs && bun run build:cljs && bun run type-check-pure",
    "type-check-pure": "NODE_OPTIONS=--max-old-space-size=8192 tsc --noEmit",
    "types:generate": "openapi-ts -f ./frontend/build/openapi/openapi-ts.config.ts",
    "types:generate:watch": "npx chokidar 'resources/openapi/openapi.json' --initial -c 'openapi-ts -f ./frontend/build/openapi/openapi-ts.config.ts' --debounce 1000",
    "validate-e2e-filenames": "node e2e/validate-e2e-test-files.js",
    "wait:cljs": "echo Waiting for first CLJS build; until [ -f target/cljs_dev/cljs.core.js ]; do sleep 1; done; echo CLJS build done",
    "generate:expression-parser": "lezer-generator frontend/src/metabase/querying/expressions/tokenizer/grammar.lezer -o frontend/src/metabase/querying/expressions/tokenizer/lezer.js",
    "generate-license-disclaimer": "node bin/generate-license-disclaimer.js",
    "cloud:dev": "bun install && clojure -A:dev:drivers:drivers-dev:ee:ee-dev:cljs -P && bun run concurrently -n 'FE,BE' -c 'blue,green' 'bun run build-hot' 'clojure -M:dev:socket:dev-start:drivers:drivers-dev:ee:ee-dev'"
  },
  "browserslist": [
    "defaults"
  ],
  "packageManager": "bun@1.3.7"
}
