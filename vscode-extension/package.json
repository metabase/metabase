{
  "publisher": "metabase",
  "name": "metastudio",
  "displayName": "Metastudio",
  "version": "0.1.0",
  "private": true,
  "packageManager": "bun@1.3.7",
  "description": "Metabase Studio extension for VS Code",
  "author": "Metabase",
  "license": "MIT",
  "homepage": "https://github.com/metabase/metabase",
  "repository": {
    "type": "git",
    "url": "https://github.com/metabase/metabase"
  },
  "bugs": {
    "url": "https://github.com/metabase/metabase/issues"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.cjs",
  "icon": "res/icon.png",
  "files": [
    "LICENSE.md",
    "dist/*",
    "res/*"
  ],
  "engines": {
    "vscode": "^1.97.0"
  },
  "activationEvents": ["onStartupFinished"],
  "contributes": {
    "commands": [
      {
        "command": "metastudio.setHost",
        "title": "Metastudio: Set Host URL",
        "icon": "$(edit)"
      },
      {
        "command": "metastudio.setApiKey",
        "title": "Metastudio: Set API Key",
        "icon": "$(edit)"
      },
      {
        "command": "metastudio.checkConnection",
        "title": "Metastudio: Check Metabase Connection",
        "icon": "$(plug)"
      },
      {
        "command": "metastudio.clearCredentials",
        "title": "Metastudio: Clear Credentials",
        "icon": "$(close)"
      },
      {
        "command": "metastudio.checkDependencyGraph",
        "title": "Metastudio: Check Dependency Graph"
      },
      {
        "command": "metastudio.showDependencyGraph",
        "title": "Metastudio: Show Dependency Graph"
      },
      {
        "command": "metastudio.showTransformPreview",
        "title": "Metastudio: Show Transform Preview"
      },
      {
        "command": "metastudio.openInMetabase",
        "title": "Open in Metabase"
      },
      {
        "command": "metastudio.showInDependencyGraph",
        "title": "Show in Dependency Graph"
      },
      {
        "command": "metastudio.runTransform",
        "title": "Run Transform",
        "icon": "$(play)"
      }
    ],
    "walkthroughs": [
      {
        "id": "metastudio-setup",
        "title": "Get Started with Metastudio",
        "description": "Connect your Metabase instance to VS Code",
        "steps": [
          {
            "id": "configure-host",
            "title": "Configure Metabase Host",
            "description": "Set the URL of your Metabase instance.\n\n[Open Settings](command:workbench.action.openSettings?%5B%22metastudio.host%22%5D)",
            "media": {
              "markdown": "media/configure-host.md"
            },
            "completionEvents": [
              "onSettingChanged:metastudio.host"
            ]
          },
          {
            "id": "set-api-key",
            "title": "Set API Key",
            "description": "Securely store your Metabase API key.\n\n[Set API Key](command:metastudio.setApiKey)",
            "media": {
              "markdown": "media/set-api-key.md"
            },
            "completionEvents": [
              "onCommand:metastudio.setApiKey"
            ]
          },
          {
            "id": "check-connection",
            "title": "Check Connection",
            "description": "Verify that Metastudio can connect to your Metabase instance.\n\n[Check Connection](command:metastudio.checkConnection)",
            "media": {
              "markdown": "media/check-connection.md"
            },
            "completionEvents": [
              "onCommand:metastudio.checkConnection"
            ]
          }
        ]
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "metabase",
          "title": "Metabase",
          "icon": "res/sidebar-icon.svg"
        }
      ]
    },
    "views": {
      "metabase": [
        {
          "id": "metabase.connection",
          "name": "Connection",
          "visibility": "collapsed"
        },
        {
          "id": "metabase.content",
          "name": "Content"
        },
        {
          "id": "metabase.dataCatalog",
          "name": "Data Catalog"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "metastudio.checkConnection",
          "when": "view == metabase.connection",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "metastudio.runTransform",
          "when": "viewItem == transform",
          "group": "inline"
        },
        {
          "command": "metastudio.setHost",
          "when": "view == metabase.connection && viewItem == connectionUrl",
          "group": "inline@1"
        },
        {
          "command": "metastudio.setApiKey",
          "when": "view == metabase.connection && viewItem == connectionToken",
          "group": "inline@1"
        },
        {
          "command": "metastudio.clearCredentials",
          "when": "view == metabase.connection && viewItem == connectionUrl",
          "group": "inline@2"
        },
        {
          "command": "metastudio.clearCredentials",
          "when": "view == metabase.connection && viewItem == connectionToken",
          "group": "inline@2"
        },
        {
          "command": "metastudio.openInMetabase",
          "when": "view == metabase.content && viewItem =~ /^(card|model|metric|dashboard|collection|transform)$/ && metastudio.hostConfigured",
          "group": "navigation@1"
        },
        {
          "command": "metastudio.openInMetabase",
          "when": "view == metabase.dataCatalog && viewItem =~ /^(database|schema|table|field)$/ && metastudio.hostConfigured",
          "group": "navigation@1"
        },
        {
          "command": "metastudio.showInDependencyGraph",
          "when": "view == metabase.content && viewItem =~ /^(card|model|metric|dashboard|collection|transform|native_query_snippet|document|action|measure|segment)$/",
          "group": "navigation@2"
        },
        {
          "command": "metastudio.showInDependencyGraph",
          "when": "view == metabase.dataCatalog && viewItem == table",
          "group": "navigation@1"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "metabase.content",
        "contents": "Open a folder with Metabase content exported via Remote Sync.",
        "when": "!metabase.configExists"
      },
      {
        "view": "metabase.dataCatalog",
        "contents": "Open a folder with Metabase content exported via Remote Sync.",
        "when": "!metabase.configExists"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Metastudio",
      "properties": {
        "metastudio.host": {
          "type": "string",
          "format": "uri",
          "default": "",
          "description": "Metabase instance URL"
        }
      }
    }
  },
  "scripts": {
    "build": "nr build:ext && nr build:webview",
    "build:ext": "tsdown src/index.ts --external vscode",
    "build:webview": "cd webview && vite build",
    "dev": "nr build:ext --watch --sourcemap",
    "dev:webview": "cd webview && vite",
    "prepare": "nr update",
    "update": "vscode-ext-gen --output src/generated/meta.ts",
    "lint": "eslint .",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "vscode:prepublish": "nr build",
    "release": "bumpp",
    "ext:package": "vsce package --no-dependencies",
    "ext:publish": "vsxpub --no-dependencies"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.7.3",
    "@antfu/ni": "^28.0.0",
    "@dagrejs/dagre": "^1.1.8",
    "@types/node": "^25.0.3",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/vscode": "^1.97.0",
    "@vitejs/plugin-react": "^4.4.0",
    "@vscode/vsce": "^3.7.1",
    "@xyflow/react": "^12.10.1",
    "bumpp": "^10.3.2",
    "eslint": "^9.39.2",
    "ovsx": "^0.10.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "reactive-vscode": "^1.0.0-beta.2",
    "taze": "^19.9.2",
    "tsdown": "^0.19.0-beta.3",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vitest": "^4.0.16",
    "vscode-ext-gen": "^1.5.1",
    "vsxpub": "^0.1.2",
    "yaml": "^2.8.2"
  }
}
