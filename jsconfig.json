{
  "compilerOptions": {
    "jsx": "react-jsx",
    "target": "esnext",
    "module": "commonjs",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "paths": {
      "*": [
        "./frontend/src/*",
        "./frontend/test/*",
        "./enterprise/frontend/src/*",
        "./enterprise/frontend/test/*"
      ],
      "cljs/*": ["./target/cljs_dev/*"]
    }
  },
  "include": [
    "frontend/src/**/*.js*",
    "enterprise/frontend/src/**/*.js*",
    "frontend/test/**/*.js*",
    "enterprise/frontend/test/**/*.js*"
  ],
  "exclude": [
    "node_modules"
  ]
}
