{
  "catalogs": [
    {
      "user": "metabase",
      "allow": "owner"
    },
    {
      "user": "employee",
      "allow": "read-only"
    }
  ],
  "tables": [
    {
      "user": "metabase",
      "privileges": [
        "SELECT",
        "INSERT",
        "DELETE",
        "UPDATE",
        "OWNERSHIP",
        "GRANT_SELECT"
      ]
    },
    {
      "user": "employee",
      "table": "employees",
      "privileges": [
        "SELECT"
      ],
      "filter": "department = 'Engineering'"
    }
  ]
}
