{
    "name": "generate-insights",
    "description": "Use this tool for exploratory data analysis, initial data exploration, or when you need a broad overview of your table, report_card, query or curated metric, including key metrics and general trends. This is a good starting point for unspecific data analysis or exploration.",
    "parameters": {
        "type": "object",
        "properties": {
            "for": {
                "anyOf": [
                    {
                        "type": "object",
                        "description": "Surface interesting patterns and insights for a pre-defined metric.",
                        "properties": {
                            "metric_id": {
                                "type": "integer",
                                "description": "The ID of the metric to analyze."
                            }
                        },
                        "required": [
                            "metric_id"
                        ],
                        "additionalProperties": false
                    },
                    {
                        "type": "object",
                        "description": "Generate insights for a table.",
                        "properties": {
                            "table_id": {
                                "type": "integer",
                                "description": "The ID of the table to analyze."
                            }
                        },
                        "required": [
                            "table_id"
                        ],
                        "additionalProperties": false
                    },
                    {
                        "type": "object",
                        "description": "Generate insights for the results of a saved report.",
                        "properties": {
                            "report_id": {
                                "type": "integer",
                                "description": "The ID of the report to analyze."
                            }
                        },
                        "required": [
                            "report_id"
                        ],
                        "additionalProperties": false
                    },
                    {
                        "type": "object",
                        "description": "Generate insights for the results of a query that recently has been run.",
                        "properties": {
                            "query_id": {
                                "type": "string",
                                "description": "The ID of the query to analyze."
                            }
                        },
                        "required": [
                            "query_id"
                        ],
                        "additionalProperties": false
                    }
                ]
            }
        },
        "required": [
            "for"
        ],
        "additionalProperties": false
    }
}
