{
  "catalogs": [
    {
      "user": "metabase",
      "allow": "owner"
    },
    {
      "user": "role_a",
      "allow": "read-only"
    },
    {
      "user": "role_b",
      "allow": "read-only"
    },
    {
      "user": "widget_role",
      "allow": "read-only"
    }
  ],
  "tables": [
    {
      "user": "metabase",
      "privileges": [
        "SELECT",
        "INSERT",
        "DELETE",
        "UPDATE",
        "OWNERSHIP",
        "GRANT_SELECT"
      ]
    },
    {
      "user": "role_a",
      "table": "venues",
      "privileges": [
        "SELECT"
      ]
    },
    {
      "user": "role_b",
      "table": "checkins",
      "privileges": [
        "SELECT"
      ]
    },
    {
      "user": "widget_role",
      "table": "products",
      "privileges": [
        "SELECT"
      ],
      "filter": "category = 'Widget'"
    }
  ]
}
