{
  "plugins": ["@emotion"],
  "presets": [
    ["@babel/preset-env", { "corejs": 3, "useBuiltIns": "usage" }],
    ["@babel/preset-react", { "runtime": "automatic" }],
    "@babel/preset-typescript"
  ],
  "env": {
    "development": {
      "presets": []
    },
    "extract": {
      "plugins": [
        [
          "ttag",
          {
            "extract": {
              "output": "locales/metabase-frontend.pot"
            },
            "discover": ["t", "jt"],
            "numberedExpressions": true
          }
        ]
      ]
    }
  }
}
